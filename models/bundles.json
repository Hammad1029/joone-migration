{
  "name": "bundles",
  "table": "bundles",
  "primaryKey": "id",
  "projections": [
    { "column": "id", "as": "id", "dataType": "number" },
    { "column": "name", "as": "name", "dataType": "string" },
    { "column": "description", "as": "description", "dataType": "string" },
    { "column": "discount", "as": "discount", "dataType": "number" },
    { "column": "status", "as": "status", "dataType": "number" },
    {
      "column": "featured_image",
      "as": "featured_image",
      "dataType": "string"
    },
    { "column": "banner_image", "as": "banner_image", "dataType": "string" },
    { "column": "deleted_at", "as": "deleted_at", "dataType": "string" }
  ]
}
